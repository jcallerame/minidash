{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": "core-js@3",
        "modules": false
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-modules-commonjs",
    [
      "@babel/plugin-transform-for-of",
      {
        "assumeArray": true
      }
    ]
  ]
}
